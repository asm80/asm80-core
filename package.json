{
  "name": "@asm80/core",
  "version": "1.0.5",
  "description": "Core ASM80 compiler / assembler",
  "main": "asm.js",
  "type": "module",
  "scripts": {
    "test": "c8 qunit && c8 report --reporter=html",
    "test:run": "c8 qunit",
    "test:report": "c8 report --reporter=html",
    "test:report:text": "c8 report --reporter=text",
    "pub": "npm version patch && git push --follow-tags && npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asm80/asm80-core.git"
  },
  "keywords": [
    "ASM",
    "Assembler",
    "Z80",
    "6502"
  ],
  "author": "Martin Maly <maly@maly.cz>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asm80/asm80-core/issues"
  },
  "homepage": "https://github.com/asm80/asm80-core#readme",
  "devDependencies": {
    "c8": "^8.0.0",
    "qunit": "^2.19.4"
  }
}
